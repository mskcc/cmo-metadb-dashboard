    "@types/node": "^16.11.43",
    "@types/react": "17.0.39",
    "@types/react-bootstrap": "^0.32.30",
    "@types/react-dom": "^17.0.17",
    "@types/react-spinkit": "^3.0.7",
    "@types/react-virtualized": "^9.21.21",
    "classnames": "^2.3.1",
    "graphql": "^16.5.0",
    "js-file-download": "^0.4.12",
    "json": "^9.0.6",
